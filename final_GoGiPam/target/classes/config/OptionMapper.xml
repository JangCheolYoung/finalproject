<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="option">
	<select id="loadOption" parameterType="java.lang.String" resultType="dto.OptionDTO">
		select * from options where item_namekey = #{item_namekey}
	</select>
  </mapper>